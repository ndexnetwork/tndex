<shortcuts>
	<skipIfNotSupported/>
	<defaultCurrentUser/>
	<programGroup defaultName="nDEX" location="startMenu"/>
	<shortcut
		name="nDEX Server"
		target="$INSTALL_PATH\nDEX.exe"
		commandLine=""
		workingDirectory="$INSTALL_PATH"
        description="Start nDEX server"
		iconFile="$INSTALL_PATH\html\www\favicon.ico"
        iconIndex="0"
        initialState="normal"
        programGroup="yes"
		desktop="yes"
        applications="no"
        startMenu="no"
		startup="no"
		runAsAdministrator="true">
	</shortcut>	
	<shortcut
		name="nDEX Wallet"
		target="http://localhost:6699/index.html"
		commandLine=""
		workingDirectory="$INSTALL_PATH"
        description="Launch nDEX wallet in browser"
		iconFile="$INSTALL_PATH\html\www\favicon.ico"
        iconIndex="0"
        initialState="normal"
        programGroup="yes"
		desktop="yes"
        applications="no"
        startMenu="no"
		startup="no">
	</shortcut>	
</shortcuts>